{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "src.server:app",
                "--reload"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "jinja": true
        }
    ]
}
